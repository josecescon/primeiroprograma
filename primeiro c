#include <stdio.h>

int main() {
    int paulo;
    int jose;
    int soma;
    int multiplicacao;
    int divisao;
    jose = 32;
    paulo = -54;
    soma = paulo + jose;
    multiplicacao = paulo * jose;
   
    printf("o valor de paulo é %d \n", paulo);
    printf("o valor de jose é %d \n", jose);
    printf("a soma de paulo e jose é %d \n", soma);
    printf("a multiplicação de paulo e jose é %d \n", multiplicacao);
    
    return 0;
}
